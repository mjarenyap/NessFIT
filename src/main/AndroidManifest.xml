<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.nessfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StatusActivity" />
        <activity android:name=".WeeklyActivity" />
        <activity android:name=".ChangeTextActivity" />
        <activity android:name=".ChangeLevelActivity" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".photoActivity" />
        <activity android:name=".ExerciseList" />
        <activity android:name=".AddExercise" />
        <activity android:name=".NotifActivity" />
    </application>

</manifest>